<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.system.mapper.SysBehaviorTypeMapper">
    
    <resultMap type="SysBehaviorType" id="SysBehaviorTypeResult">
        <result property="behaviorTypeId"    column="behavior_type_id"    />
        <result property="behaviorCode"    column="behavior_code"    />
        <result property="behaviorName"    column="behavior_name"    />
        <result property="behaviorCategory"    column="behavior_category"    />
        <result property="description"    column="description"    />
        <result property="detectionMethod"    column="detection_method"    />
        <result property="aiModelVersion"    column="ai_model_version"    />
        <result property="confidenceThreshold"    column="confidence_threshold"    />
        <result property="isActive"    column="is_active"    />
        <result property="sortOrder"    column="sort_order"    />
        <result property="colorCode"    column="color_code"    />
        <result property="iconUrl"    column="icon_url"    />
